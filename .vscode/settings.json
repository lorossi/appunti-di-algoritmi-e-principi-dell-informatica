{
  "latex-workshop.latex.recipes": [
    {
      "name": "build with externalized images",
      "tools": ["envsetup", "pdflatex", "pdflatex"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "envsetup",
      "command": "python3",
      "args": ["envsetup.py"],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    }
  ],
  "latex-workshop.latex.recipe.default": "build with externalized images",
  "latex-workshop.latex.autoBuild.interval": 5000,
  "latex-workshop.message.warning.show": false,
  "latex-workshop.message.error.show": false,
  "latex-workshop.view.outline.sections": [
    "part",
    "chapter",
    "section",
    "subsection",
    "subsubsection",
    "paragraph"
  ],
  "latex-workshop.latexindent.args": [
    "-c",
    "%DIR%/",
    "%TMPFILE%",
    "-y=defaultIndent: '%INDENT%'",
    "-m"
  ],
  "[latex]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "algoritmicamente",
    "arietà",
    "biiettivamente",
    "biiezione",
    "Büchi",
    "contronominale",
    "Cormen",
    "dall",
    "deque",
    "derivabilità",
    "diagonalizzazione",
    "downto",
    "Elgot",
    "formalizzabili",
    "Georg",
    "Gödelizzazione",
    "Gottlob",
    "HEAPIFY",
    "HEAPSORT",
    "Hoare",
    "idempotenza",
    "INORDER",
    "insoddisfacibile",
    "insoddisfacibilità",
    "irrisolvibilità",
    "Kleene",
    "L'indefinizione",
    "Leiserson",
    "Mandrioli",
    "Martinenghi",
    "monadica",
    "monadici",
    "monoide",
    "MULT",
    "multinastro",
    "Naur",
    "nullari",
    "nullaria",
    "nullarie",
    "nullario",
    "opzionalità",
    "polimi",
    "polinomialmente",
    "POSIX",
    "Postcondizione",
    "postcondizioni",
    "POSTORDER",
    "PREORDER",
    "pseudocodice",
    "pseudocodici",
    "Pushdown",
    "semidecidibile",
    "semidecisione",
    "semigruppo",
    "soddisfacibilità",
    "sottostringa",
    "sottostringhe",
    "Trakhtenbrot",
    "tupla"
  ],
  "cSpell.ignoreWords": [
    "aabb",
    "bcba",
    "bcbb",
    "bcbc",
    "bcca",
    "bccb",
    "bccc",
    "bleah",
    "caba",
    "cabb",
    "cabc",
    "caca",
    "cacb",
    "cacc",
    "yyyyxx"
  ]
}
